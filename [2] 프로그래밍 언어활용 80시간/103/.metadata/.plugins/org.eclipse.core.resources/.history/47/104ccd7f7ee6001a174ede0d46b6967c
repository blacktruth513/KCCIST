class BankAccount {
	String accNumber; // 계좌번호
	String ssNumber; // 주민번호
	int balance = 0; // 예금잔액
	
	// 초기화를 위한 메소드
	public void initAccount(String acc, String ss, int bal) {
		accNumber = acc;
		ssNumber = ss;
		balance = bal;  // 계좌 개설 시 예금액으로 초기화
	}
}
public class p169 {

	public static void main(String[] args) {


	}

}
