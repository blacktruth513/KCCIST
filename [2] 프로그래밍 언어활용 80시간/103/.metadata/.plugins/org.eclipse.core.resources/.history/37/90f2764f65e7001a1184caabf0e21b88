package com.test;

public class Test11 {

	public static void main(String[] args) {
		// int[] a = {1,2,3};
		// for(int i = 0; i <= a.length; i++) {
		// System.out.println(a[i]);
		// }
		int[][] arr = new int[2][2];
		arr[0][0] = 1;
		arr[1][0] = 2;
		arr[0][1] = 3;
		arr[1][1] = 4;
		// System.out.println(arr.length);
		for (int i = 0; i < 2; i++) {
			// System.out.println(arr[i][0] + "," + arr[i][1]);
			for (int j = 0; j < 2; j++) {
				System.out.println(arr[i][j]);
			}
		}
	}

}
