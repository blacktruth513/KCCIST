package com.test19;
interface Database {
	void open();
	void close();
}
abstract class RDBMS implements Database {
public abstract void open(); {System.out.println("RDBMS");}
}
class Oracle extends RDBMS {}
	public void open() {System.out.println("Oracle");}
}
public class Test1 {
	public static void main(String[] args) {
		Oracle o = new Oracle();
		o.open();
	Database d = new Oracle();
	}
}
